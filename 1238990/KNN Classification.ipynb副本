   "source": [
    "# cheat to get the same \"random\" numbers\n",
    "np.random.seed(seed=99)\n",
    "\n",
    "# create some data points\n",
    "mean1 = [3,3]\n",
    "mean2 = [8,8]\n",
    "cov = [[1.0,0.0],[0.0,1.0]] \n",
    "\n",
    "x1 = np.random.multivariate_normal(mean1,cov,50)\n",
    "x2 = np.random.multivariate_normal(mean2,cov,50)\n",
    "\n",
    "plt.scatter(x1[:,0],x1[:,1], c='r', s=100)\n",
    "plt.scatter(x2[:,0],x2[:,1], c='b', s=100)\n",
    "\n",
    "plt.plot([2,10],[10,1], c='g', linewidth=5.0)\n",
    "\n",
    "plt.title(\"Prediction in One Picture\")\n",
    "plt.xlabel(\"Feature 1\")\n",
    "plt.ylabel(\"Feature 2\")\n",
    "\n",
    "fig_prediction = plt.gcf()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "# merge the data\n",
    "x = np.concatenate((x1, x2))\n",
    "\n",
    "# create the labels\n",
    "ones = np.ones((50,))\n",
    "y = np.concatenate((ones*0, ones))"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
