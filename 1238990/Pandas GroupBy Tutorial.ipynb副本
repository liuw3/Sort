       "    </tr>\n",
       "    <tr>\n",
       "      <th>1992</th>\n",
       "      <td>425000</td>\n",
       "    </tr>\n",
       "  </tbody>\n",
       "</table>\n",
       "</div>"
      ],
      "text/plain": [
       "      Salary\n",
       "Year        \n",
       "1988  235000\n",
       "1989  300000\n",
       "1990  190000\n",
       "1991  500000\n",
       "1992  425000"
      ]
     },
     "execution_count": 9,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "mlb_by_year.agg(np.median).head()"
   ]
